{
    "name": "DemacMedia/oro-livechat-integration-bundle",
    "description": "LivechatInc Integration to use in OroCRM",
    "homepage": "https://github.com/DemacMedia/OroLivechatIntegrationBundle.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Demac Media",
            "homepage": "http://www.demacmedia.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "DemacMedia\\Bundle\\OroLivechatIntegrationBundle": "src/"
        }
    },
    "target-dir": "src/DemacMedia/Bundle/OroLivechatIntegrationBundle",
    "require": {
        "php": ">=5.4.9",
        "oro/platform": "1.*",
        "oro/crm": "1.*",
        "livechat/api-client-php": "@dev"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}

